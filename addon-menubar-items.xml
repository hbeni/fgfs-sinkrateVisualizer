<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
  <meta>
    <file-type type="string">FlightGear add-on menu bar items</file-type>
    <format-version type="int">1</format-version>
  </meta>

  <menubar-items>
        <menu>
            <label>SinkrateVisualizer</label>
            <enabled type="bool">true</enabled>

            <item>
                <label>Open/Close Sinkrate Map</label>
                <binding>
                    <command>nasal</command>
                    <script><![CDATA[SinkrateVisualizer.openMap();]]></script>
                </binding>
            </item>

        </menu>
    </menubar-items>

</PropertyList>
